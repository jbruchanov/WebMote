<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:com.scurab.web.remotecontrol.client.controls"
	>
	<ui:style>
		.loader
		{
			margin: 2px;
			margin-top:15px;
		}
	</ui:style>
	<g:HTMLPanel >
		<g:HorizontalPanel width="" horizontalAlignment="ALIGN_CENTER">
			<my:ImageMobileButton url="pics/btn_mc_write.png" altText="Cust" ui:field="btnStartCustomProcess" />
			<my:ImageMobileButton url="pics/btn_refresh.png" altText="Reload" ui:field="btnReload" />
			<g:Image altText="l" ui:field="imgLoader" url="pics/ajax-loader.gif" styleName="gwt-Image {style.loader}"/>
		</g:HorizontalPanel>
		<g:HTMLPanel ui:field="contentPanel"/>
	</g:HTMLPanel>
</ui:UiBinder> 