<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.parent
		{
			position: static;
			left: 150px;

		}
		
		.containers
		{
		}
		
		.left
		{
			background-image: url("pics/cp_left_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 140px;
			z-index: 100;
		/*	border: solid 1px #FFF;*/
			
		}
		.up
		{
			background-image: url("pics/cp_up_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 160px;
			height: 80px;
			z-index: 100;
			/*border: solid 1px #FFF;*/
			
		}
		.right
		{
			background-image: url("pics/cp_right_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 140px;
			z-index: 100;
			/*border: solid 1px #FFF;*/
		}
		.down
		{
			background-image: url("pics/cp_down_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 160px;
			height: 80px;
			z-index: 100;
		/*	border: solid 1px #FFF;*/
		}
		.center
		{
			background-image: url("pics/cp_stop_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 140px;
			height: 140px;
			z-index: 100;
		/*	border: solid 1px #FFF;*/
		}
		
		
		</ui:style>
		<g:VerticalPanel styleName="{style.parent}" width="100%" height="" horizontalAlignment="ALIGN_CENTER">
			<g:AbsolutePanel width="300px" height="300px">
				<g:at left="0" top="0">
					<g:Image url="pics/cp_type2_background.png"/>
				</g:at>
				<g:at left="0" top="80">
					<g:InlineLabel styleName="{style.left} unselectable"  ui:field="btnLeft"/>
				</g:at>
				<g:at left="70" top="0">
					<g:InlineLabel styleName="{style.up} unselectable" ui:field="btnUp"  />
				</g:at>
				<g:at left="220" top="80">
					<g:InlineLabel styleName="{style.right} unselectable" ui:field="btnRight"  />
				</g:at>
				<g:at left="70" top="220">
					<g:InlineLabel styleName="{style.down} unselectable" ui:field="btnDown"  />
				</g:at>
				<g:at left="80" top="80">
					<g:InlineLabel styleName="{style.center} unselectable" ui:field="btnCenter"  />
				</g:at>
			</g:AbsolutePanel>
		</g:VerticalPanel>
</ui:UiBinder> 