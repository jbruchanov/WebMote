<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.containers
		{
			position: absolute;
		}
		
		.minus
		{
			background-image: url("pics/cp_minus_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 60px;
			height: 60px;
			z-index: 100;
		}
		
		.plus
		{
			background-image: url("pics/cp_plus_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 60px;
			height: 60px;
			z-index: 100;
		}
		
		.speaker
		{
			background-image: url("pics/cp_speaker_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 70px;
			height: 70px;
			z-index: 100;
		}
		
		.stop
		{
			background-image: url("pics/cp_stop_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 60px;
			height: 60px;
			z-index: 100;
		}
		.pause
		{
			background-image: url("pics/cp_pause_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 60px;
			height: 60px;
			z-index: 100;
		}
		.play
		{
			background-image: url("pics/cp_play_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 120px;
			height: 70px;
			z-index: 100;
		}
		
		.seekback
		{
			background-image: url("pics/cp_seekb_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 80px;
			z-index: 100;
		}
		
		.skipback
		{
			background-image: url("pics/cp_skipb_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 60px;
			z-index: 100;
		}
		
		.seekforward
		{
			background-image: url("pics/cp_seekf_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 80px;
			top: 5px;
			z-index: 100;
		}
		
		.skipnext
		{
			background-image: url("pics/cp_skipn_normal.png");
			background-repeat: no-repeat;
			background-position:center; 
			width: 80px;
			height: 60px;
			z-index: 100;
		}
		
	</ui:style>
	
	<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER" height="100%" verticalAlignment="ALIGN_TOP">
		<g:AbsolutePanel width="300px" height="300px">
			<g:at left="0" top="0">
					<g:Image url="pics/cp_type1_background.png"/>
			</g:at>
			<g:at left="50" top="150">
				<g:InlineLabel styleName="{style.minus} unselectable"  ui:field="btnMinus"/>
			</g:at>
			<g:at left="200" top="150">
				<g:InlineLabel styleName="{style.plus} unselectable"  ui:field="btnPlus"/>
			</g:at>
			<g:at left="115" top="160">
				<g:InlineLabel styleName="{style.speaker} unselectable"  ui:field="btnMute"/>
			</g:at>
			<g:at left="152" top="92">
				<g:InlineLabel styleName="{style.stop} unselectable"  ui:field="btnStop"/>
			</g:at>
			<g:at left="88" top="92">
				<g:InlineLabel styleName="{style.pause} unselectable"  ui:field="btnPause"/>
			</g:at>
			<g:at left="90" top="18">
				<g:InlineLabel styleName="{style.play} unselectable"  ui:field="btnPlay"/>
			</g:at>
			<g:at left="15" top="5">
				<g:InlineLabel styleName="{style.seekback} unselectable"  ui:field="btnSeekBack"/>
			</g:at>
			<g:at left="205" top="18">
				<g:InlineLabel styleName="{style.seekforward} unselectable"  ui:field="btnSeekForward"/>
			</g:at>
			<g:at left="10" top="90">
				<g:InlineLabel styleName="{style.skipback} unselectable"  ui:field="btnPrevious"/>
			</g:at>
			<g:at left="210" top="90">
				<g:InlineLabel styleName="{style.skipnext} unselectable"  ui:field="btnNext"/>
			</g:at>
			</g:AbsolutePanel>
	</g:VerticalPanel>
</ui:UiBinder> 