<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.procName
		{
			font-weight: bold;
			font-size: 1.2em;
		}
		
		.procId
		{
			position:relative;
			font-size: 1.1em;
			text-align: right;
			right: 2px;
		}
		
		.procDescription
		{
			margin-top:2px;
			font-size: 0.9em;
			color: #606060;
		}
		
		.container
		{
			min-height: 40px;
		}
	</ui:style>
	<g:FocusPanel ui:field="container" styleName="{style.container} gwt-Button">
		<g:VerticalPanel width="100%" >
			<g:HorizontalPanel width="100%">
				<g:Label text="ProcessName" width="85%" ui:field="lblProcessName" styleName="{style.procName}"/>
				<g:HorizontalPanel width="100%" horizontalAlignment="ALIGN_LEFT">
					<g:Label text="ProcessId" width="100%" ui:field="lblProcessId" styleName="{style.procId}" horizontalAlignment="ALIGN_RIGHT"/>
				</g:HorizontalPanel>
			</g:HorizontalPanel>
			<g:Label text="ProcessDescription" ui:field="lblProcessDescription" styleName="{style.procDescription}"/>
		</g:VerticalPanel>
	</g:FocusPanel>
</ui:UiBinder> 